{
  "name": "masar-qatar-personal-finance",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "preinstall": "node -e \"if(!/^20\\./.test(process.versions.node)){console.error('‚ùå Use Node 20.x (nvm use 20)');process.exit(1)}\"",
    "start": "expo start",
    "start:ios": "expo start --ios",
    "start:android": "expo start --android",
    "start:dev": "expo start --dev-client",
    "dev:api": "tsx backend/server.ts",
    "migrate": "prisma migrate dev --name init --create-db",
    "generate": "prisma generate",
    "dev:all": "run-p dev:api start",
    "build:api": "tsc -p backend/tsconfig.json",
    "start:api": "node backend/dist/server.js",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:all": "eas build --platform all",
    "preview:ios": "eas build --platform ios --profile preview",
    "preview:android": "eas build --platform android --profile preview",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "prepush:guard": "./scripts/healthcheck.sh",
    "typecheck": "tsc -p backend/tsconfig.json --noEmit && tsc -p tsconfig.json --noEmit",
    "prepare": "husky"
  },
  "engines": {
    "node": "20.x"
  },
  "overrides": {
    "expo": "53.0.20",
    "expo-blur": "14.1.5",
    "expo-constants": "17.1.7",
    "expo-font": "13.3.2",
    "expo-linear-gradient": "14.1.5",
    "expo-linking": "7.1.7",
    "expo-location": "18.1.6",
    "expo-splash-screen": "0.30.10",
    "expo-system-ui": "5.0.10",
    "react-dom": "19.0.0",
    "react-native-web": "0.20.0",
    "@react-native-community/datetimepicker": "8.4.1"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@hono/node-server": "^1.18.1",
    "@hono/trpc-server": "^0.4.0",
    "@nkzw/create-context-hook": "^1.1.0",
    "@prisma/client": "^6.13.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/native": "^7.1.6",
    "@tanstack/react-query": "^5.84.1",
    "@trpc/client": "^11.4.4",
    "@trpc/react-query": "^11.4.4",
    "@trpc/server": "^11.4.4",
    "bcryptjs": "^2.4.3",
