    "expo-router": "~5.1.4",
    "expo-sqlite": "~15.2.14",
    "expo-status-bar": "~2.2.3",
    "hono": "^4.8.12",
    "jsonwebtoken": "^9.0.2",
    "lucide-react-native": "^0.539.0",
    "nanoid": "^5.1.5",
    "prisma": "^6.13.0",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-safe-area-context": "^5.4.0",
